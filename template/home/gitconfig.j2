[user]
	email = {{ dotfiles_git_user_email }}
	name = {{ dotfiles_git_user_name }}
[color]
	ui = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[core]
	editor = /usr/bin/vim
